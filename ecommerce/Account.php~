<?php
class Account{
	private $id;
	private $name;
	private $vorname;
	private $username;
	private $password;
	private $rolle;
	
	public function __construct($id=null,$name=null,$vorname=null, $username=null,$password=null, $rolle=null){
			$this->id = $id;
			$this->name = $name;
			$this->vorname = $vorname;
			$this->username = $username;
			$this->password = $password;
			$this->rolle = $rolle;
	}

	
		
	
	
}

?>